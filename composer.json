{
  "name": "bobbyahines/lamp-scaffolding",
  "description": "An MVP web app scaffolding on which to quickly prototype new applications.",
  "type": "library",
  "license": "proprietary",
  "authors": [
    {
      "name": "Bobby Hines",
      "email": "bobbyahines@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "require": {
    "php": ">=8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-gd": "*",
    "ext-mysqli": "*",
    "vlucas/phpdotenv": "^5.3",
    "twig/twig": "^3.3",
    "league/route": "^5.1",
    "laminas/laminas-diactoros": "^2.8",
    "laminas/laminas-httphandlerrunner": "^2.1"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
